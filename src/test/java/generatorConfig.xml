<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE generatorConfiguration 
PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" 
"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<properties resource="jdbc.properties" />
	<context id="testTables" targetRuntime="MyBatis3">
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
			<!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
		</commentGenerator>
		<!-- 数据库连接信息 -->
		<jdbcConnection driverClass="${jdbc.driver}"
			connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}" />
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 生成pojo类的位置 -->
		<javaModelGenerator targetPackage="com.fafu.dormitorymanage.pojo" targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- 生成mapper映射文件位置 -->
		<sqlMapGenerator targetPackage="com.fafu.dormitorymanage.mapping"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<!-- 生成mapper接口位置 -->
		<javaClientGenerator targetPackage="com.fafu.dormitorymanage.dao" type="XMLMAPPER" targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<table schema="general" tableName="out_entry_record" domainObjectName="OutEntryRecord"
		    enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"
		    enableSelectByExample="false" selectByExampleQueryId="false" >
		    <property name="useActualColumnNames" value="true"/>
		</table>
	</context>
</generatorConfiguration>